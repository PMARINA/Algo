
public class Node2LL<Value> {
	Node2LL left;
	Node2LL right;
	double x;
	double y;
	int useXAxis;
	Value v;
	public Node2LL(double x, double y, int useXAxis, Node2LL left, Node2LL right, Value v){
		this.left = left;
		this.right = right;
		this.useXAxis = useXAxis;
		this.x = x;
		this.y = y;
		this.v = v;
	}
}
